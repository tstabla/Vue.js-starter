{
  "name": "Vue.js-starter",
  "version": "1.0.0",
  "author": {
    "name": "Tomasz Stabla",
    "email": "t.stabla@hotmail.com",
    "url": "http://stabla.com"
  },
  "private": true,
  "scripts": {
    "clean": "find 'web/assets/js' 'web/assets/css' -type f -not -name '.gitkeep' | xargs rm && rm -rf web/assets/cached_uglify",
    "build": "npm run clean && webpack --config webpack.config.js",
    "prod": "export NODE_ENV=production && npm run build",
    "dev": "export NODE_ENV=development && nodemon --watch web --ext html server.dev.js",
    "view": "npm run build && nodemon --watch web --ext html server.prod.js"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-loader": "^6.4.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "body-parser": "^1.17.0",
    "bundle-loader": "^0.5.4",
    "css-loader": "^0.26.1",
    "express": "^4.15.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.2",
    "file-loader": "^0.10.0",
    "html-loader": "^0.4.4",
    "json-loader": "^0.5.4",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "lodash": "^4.13.1",
    "matchdep": "*",
    "node-sass": "^4.4.0",
    "nodemon": "^1.11.0",
    "postcss-assets": "^4.1.0",
    "postcss-loader": "^1.2.2",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1",
    "webpack-uglify-js-plugin": "^1.1.9"
  }
}
